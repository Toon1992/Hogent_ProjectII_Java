                    <ComboBox fx:id="cmbMateriaal" maxWidth="-Infinity" prefHeight="31.0" prefWidth="160.0" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
