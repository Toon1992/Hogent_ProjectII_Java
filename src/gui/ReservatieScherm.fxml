                  <TableColumn fx:id="BeginDatumColumn" minWidth="250.0" prefWidth="250.0" styleClass="table-Column" text="Begin Datum" />
                  <TableColumn fx:id="EindDatumColumn" maxWidth="1.7976931348623157E308" minWidth="250.0" prefWidth="250.0" styleClass="table-Column" text="Eind Datum" />
              </columns>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <GridPane minHeight="281.0" prefHeight="281.0" VBox.vgrow="ALWAYS">
