                            <Insets />
                        </GridPane.margin>
                    </ComboBox>
                    <ComboBox fx:id="cmbNaam" prefWidth="150.0" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
                        <GridPane.margin>
                            <Insets right="75.0" />
                        </GridPane.margin></ComboBox>
                    <Label text="Aantal:" GridPane.columnIndex="3" GridPane.rowIndex="4" />
                    <TextField fx:id="txfAantal" maxWidth="150.0" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="4" GridPane.valignment="CENTER" />
                    <Button mnemonicParsing="false" onAction="#nieuweReservatie" text="Nieuw" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="6" />
                </children>
                <padding>
                    <Insets left="10.0" right="10.0" />
                </padding>
