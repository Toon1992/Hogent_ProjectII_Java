<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<fx:root type="HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="650.0" prefWidth="1200.0" stylesheets="@../styleSheet/style.css">
         <children>
            <GridPane>
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" percentWidth="1.0" prefWidth="1.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="215.0" minWidth="10.0" prefWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="385.0" minWidth="10.0" prefWidth="150.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="10.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" percentWidth="5.0" prefWidth="1.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label alignment="CENTER" text="Zoek:" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <TextField fx:id="txfZoek" onKeyTyped="#zoeken" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Label alignment="CENTER" text="BeginDatum:" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <Label alignment="CENTER" text="EindDatum" GridPane.columnIndex="5" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <DatePicker fx:id="datePickerBegin" onAction="#zoekOpBeginDatum" prefHeight="31.0" prefWidth="257.0" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                  <DatePicker fx:id="datePickerEind" onAction="#zoekOpEindDatum" prefHeight="31.0" prefWidth="257.0" GridPane.columnIndex="6" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <TableView id="my-table" fx:id="reservatieTable" minHeight="-Infinity" prefHeight="500.0" prefWidth="1200.0" stylesheets="@../styleSheet/style.css" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="naamColumn" prefWidth="250.0" styleClass="table-Column" text="Naam" />
                <TableColumn fx:id="materiaalColumn" minWidth="0.0" prefWidth="250.0" styleClass="table-Column" text="Materiaal" />
                  <TableColumn fx:id="aantalColumn" minWidth="0.0" prefWidth="110.0" styleClass="table-Column" text="Aantal" />
                  <TableColumn fx:id="statusColumn" minWidth="0.0" prefWidth="200.0" styleClass="table-Column" text="Status" />
                  <TableColumn fx:id="BeginDatumColumn" minWidth="0.0" prefWidth="192.0" styleClass="table-Column" text="Begin Datum" />
                  <TableColumn fx:id="EindDatumColumn" maxWidth="1.7976931348623157E308" minWidth="0.0" prefWidth="195.0" styleClass="table-Column" text="Eind Datum" />
              </columns>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </TableView>
         </children>
      </VBox>
   </children>
</fx:root>
