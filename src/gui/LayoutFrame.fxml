               <image>
                  <Image url="@../images/home.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btnCatalogus" fitHeight="40.0" fitWidth="40.0" onMouseClicked="#gaNaarCatalogus" pickOnBounds="true" preserveRatio="true" styleClass="StartButton" GridPane.columnIndex="2" GridPane.halignment="CENTER">
               <image>
                  <Image url="@../images/catalogus.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btnReserveren" fitHeight="40.0" fitWidth="40.0" onMouseClicked="#gaNaarReservaties" pickOnBounds="true" preserveRatio="true" styleClass="StartButton" GridPane.columnIndex="4" GridPane.halignment="CENTER">
               <image>
                  <Image url="@../images/reservaties.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btnBeheerder" fitHeight="40.0" fitWidth="40.0" onMouseClicked="#gaNaarBeheerders" pickOnBounds="true" preserveRatio="true" styleClass="StartButton" GridPane.columnIndex="3" GridPane.halignment="CENTER">
